{
    "name": "boxed-code/laravel-scout-database",
    "description": "Generic database driver for laravel scout.",
    "keywords": ["laravel", "scout", "database", "mysql", "sqlite"],
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Green",
            "email": "oliver@boxedcode.co.uk"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/scout": "^8.0"
    },
     "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.5"
    },
    "autoload": {
        "psr-4": {
            "BoxedCode\\Laravel\\Scout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BoxedCode\\Laravel\\Scout\\DatabaseEngineServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
